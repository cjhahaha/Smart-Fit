<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <!-- Customize your shape here, e.g., rounded corners -->
            <solid android:color="@android:color/transparent" /> <!-- or any color you need -->
            <corners android:radius="8dp"/> <!-- Set corner radius if needed -->
            <stroke
                android:width="1dp"
                android:color="@android:color/transparent" /> <!-- Use transparent color to remove underline -->
        </shape>
    </item>
</selector>